{
  "name": "azure-plugin",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "start": "static build -p 9090 -H '{\"Access-Control-Allow-Origin\": \"*\",\"Access-Control-Allow-Methods\": \"*\",\"Access-Control-Allow-Headers\": \"*\", \"Cache-Control\": \"no-cache, no-store, must-revalidate\"}'",
    "eslint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "stylelint": "stylelint \"./**/*.scss\"",
    "lint": "npm run eslint",
    "format:js": "npm run eslint -- --fix",
    "format:scss": "npm run stylelint -- --fix",
    "format": "npm run format:js && npm run format:scss",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
